{
  "name": "deffered-patch",
  "version": "0.0.1",
  "description": "Deffered virtual dom patching based on Promisses",
  "main": "index.js",
  "keywords": [
    "virtual-dom",
    "patch",
    "promisses",
    "promise",
    "lazy",
    "deffered",
    "render"
  ],
  "author": "Damian Baar <damian.baar@gmail.com> (http://github.com/damianbaar)",
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.10.1",
    "seamless-immutable": "^4.0.2",
    "vdom-thunk": "^3.0.0",
    "virtual-dom": "^2.1.1",
    "virtual-hyperscript-svg": "^2.0.0",
    "x-is-array": "^0.1.0"
  },
  "watch": {
    "test": "{vdom,test}/**/*.js"
  },
  "scripts": {
    "test": "node node_modules/tape/bin/tape test/*.js --render=dots",
    "dev": "node node_modules/npm-watch/cli.js",
    "example": "browserify example/index.js -o example/bundle.js -d",
    "example-dev": "watchify example/index.js -o example/bundle.js -d"
  },
  "devDependencies": {
    "cssauron": "^1.2.0",
    "npm-watch": "0.0.1",
    "sinon": "^1.17.2",
    "tape": "^4.2.2",
    "velocity-animate": "^1.2.3",
    "vtree-select": "^2.0.0"
  }
}
